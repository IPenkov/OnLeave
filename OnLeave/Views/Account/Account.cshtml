@{
    ViewBag.Title = "Account";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section LeftSideBar{
    @{Html.RenderPartial("AccounLeftSideBar");}
}


<div class="breadcrumb">
    @Html.ActionLink("Начало", "Index", "Home")
    » @Html.ActionLink("Профил", "Login", "Account", new { @class = "last" })
</div>

<h1>Моят профил</h1>
<div class="box-container">
    <h2>Профил</h2>

    <div class="content">
        <ul>
            <li>@Html.ActionLink("Промяна профил", "EditAccount", "Account")</li>
            <li>@Html.ActionLink("Смяна парола", "ChangePassword", "Account")</li>            
        </ul>
    </div>
</div>


